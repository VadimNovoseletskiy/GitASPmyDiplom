    </form>

    <form action="/AdminLand/Index">
        <button type="submit">Показати</button>
        <label>Список об'єктів"Земля"</label>
    </form>
    
    <form action="/AdminHouse/Index">
        <button type="submit">Показати</button>
        <label>Список об'єктів"Будинки"</label>
    </form>

    <form action="/AdminApartment/Index">
        <button type="submit">Показати</button>
        <label>Список об'єктів"Квартири"</label>
    </form>

    <form action="/AdminCommercial/Index">
        <button type="submit">Показати</button>
        <label>Список об'єктів"Комерційна нерухомість "</label>
    </form>
</fieldset>

